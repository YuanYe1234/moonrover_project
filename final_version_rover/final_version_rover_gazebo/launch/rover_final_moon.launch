<launch>
  <arg name="gui" default="true"/>
  <arg name="INITIAL_POSE_X" default="10" />
  <arg name="INITIAL_POSE_Y" default="10" />
  <arg name="INITIAL_POSE_Z" default="0.5" />
  <include file="$(find final_version_rover_gazebo)/launch/include/gazebo.launch.xml" >
    <arg name="model" value="$(find final_version_rover_description)/urdf/rover_final.urdf.xacro"  />
    <arg name="world_name" default="$(find final_version_rover_gazebo)/worlds/apollo15_landing_site.world"/>
    <arg name="gui" value="$(arg gui)" />
    <arg name="INITIAL_POSE_X" value="$(arg INITIAL_POSE_X)" />
    <arg name="INITIAL_POSE_Y" value="$(arg INITIAL_POSE_Y)" />
    <arg name="INITIAL_POSE_Z" value="$(arg INITIAL_POSE_Z)" />
  </include>
</launch>